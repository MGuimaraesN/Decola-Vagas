datasource db {
  provider = "mysql" // Verifique se est√° "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  email     String   @unique
  password  String
  ip        String
  lastLogin DateTime?
  updatedAt DateTime @updatedAt
  createdAt DateTime @default(now())
}